{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "CarMakeAndModel": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "FirstName": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "LastName": {
      "type": "string",
      "minLength": 1,
      "required": true
    },
    "Phone": {
      "type": "biginteger",
      "required": true
    },
    "SelectService": {
      "type": "enumeration",
      "enum": [
        "Standard Car Wash & Clean",
        "Tire Care & Wheel Alignment",
        "PPF & Ceramic Coating",
        "Service & Maintenance",
        "Engine & Transmission Repair",
        "Custom Modificatoins & Accessories"
      ],
      "required": true
    }
  }
}
